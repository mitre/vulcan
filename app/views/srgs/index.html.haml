- content_for :assets do
  = javascript_pack_tag 'upload'

%h1 Srgs#index
%h3 Upload new SRG

#upload
  = form_tag(srgs_path, multipart: true) do
    .input-group
      %b-form-file{:name => "out_item[images][]", :placeholder => "Choose SRG"}

    %br/
    .actions
      = submit_tag 'Upload', :class => "btn btn-primary"
